#!/bin/bash

rm -rf *.csv
echo "################ AgentScript ###############################"
cd agentscript || exit
rm -rf C*
rm -rf *.csv
cd ..
echo "################ Transpiled AgentScript ####################"
cd agentscript_compiled || exit
rm -rf *.csv
cd ..
echo "################ SARL ######################################"
cd sarl || exit
rm -rf *.csv
cd ..
echo "################ ASTRA #####################################"
cd astra || exit
rm -rf *.csv
cd ..
echo "################ JASON #####################################"
cd jason || exit
rm -rf C*
rm -rf *.csv
cd ..